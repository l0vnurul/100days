
public class Day_13 {
    public static void main(String[]args) {
        int nilaiE = 18;
        int nilaiD = 4;
        boolean hasil;
        
        // Apakah D lebih besar dari E?
        hasil = nilaiD > nilaiE; 
        System.out.println(hasil);
        
        
        // Apakah D lebih kecil dari E?
        hasil = nilaiD < nilaiE;
        System.out.println(hasil);
        
        // Apakah A lebih besar sama dengan e?
        hasil = nilaiD >= nilaiE;
        System.out.println(hasil);
        
        // Apakah D lebih kecil sama dengan E?
        hasil = nilaiD <= nilaiE;
        System.out.println(hasil);
        
        // Apakah nilai D tidak sama dengan E?
        hasil = nilaiD != nilaiE;
        System.out.println(hasil);
        
    }
}
